-- A. åœ°åœ–å…¨äº®æ§åˆ¶å¾ªç’°
task.spawn(function()
    local Lighting = game:GetService("Lighting")
    -- ç´€éŒ„åŸå§‹è¨­å®šï¼Œæ–¹ä¾¿é—œé–‰æ™‚æ¢å¾©
    local oldAmbient = Lighting.Ambient
    local oldOutdoorAmbient = Lighting.OutdoorAmbient
    local oldFogEnd = Lighting.FogEnd
    
    while true do
        if _G.FullBright then
            Lighting.Ambient = Color3.new(1, 1, 1)
            Lighting.OutdoorAmbient = Color3.new(1, 1, 1)
            Lighting.FogEnd = 999999
            Lighting.GlobalShadows = false
        else
            -- é€™è£¡ä¸å¼·è¡Œæ¢å¾©åŸå§‹å€¼ï¼Œå› ç‚ºä¸åŒéŠæˆ²è¨­å®šä¸åŒ
            -- ä½†é—œé–‰æ™‚æˆ‘å€‘æœƒæŠŠäº®åº¦èª¿å›ä¸€å€‹åˆç†çš„ç¯„åœï¼Œæˆ–è€…ä½ å¯ä»¥é‡æ–°è¼‰å…¥
            Lighting.GlobalShadows = true
        end
        task.wait(1)
    end
end)

-- B. é›™é–‹é—œ ESP é‚è¼¯
task.spawn(function()
    local Players = game:GetService("Players")
    local LocalPlayer = Players.LocalPlayer
    while task.wait(0.4) do
        if _G.ShowESP_ID or _G.ShowESP_Health then
            for _, player in pairs(Players:GetPlayers()) do
                pcall(function()
                    if player ~= LocalPlayer and player.Character then
                        local head = player.Character:FindFirstChild("Head")
                        local hum = player.Character:FindFirstChild("Humanoid")
                        if head and hum then
                            local tag = player.Character:FindFirstChild("LunaSplitTag")
                            if not tag then
                                tag = Instance.new("BillboardGui", player.Character)
                                tag.Name = "LunaSplitTag"; tag.Adornee = head
                                tag.Size = UDim2.new(0, 250, 0, 80); tag.AlwaysOnTop = true
                                tag.ExtentsOffset = Vector3.new(0, 4, 0)
                                local label = Instance.new("TextLabel", tag)
                                label.Name = "InfoLabel"; label.BackgroundTransparency = 1; label.Size = UDim2.new(1, 0, 1, 0)
                                label.TextColor3 = Color3.new(1, 1, 1); label.TextStrokeTransparency = 0
                                label.Font = Enum.Font.SourceSansBold; label.TextSize = 22; label.RichText = true
                            end
                            local content = ""
                            local dist = math.floor((LocalPlayer.Character.HumanoidRootPart.Position - head.Position).Magnitude)
                            if _G.ShowESP_ID then content = content .. player.DisplayName .. " [" .. dist .. "m]" end
                            if _G.ShowESP_Health then
                                local hp = math.floor(hum.Health); local maxHp = math.floor(hum.MaxHealth); local percent = (hp / maxHp) * 100
                                local hpColor = (percent > 60 and "0,255,0") or (percent > 30 and "255,255,0") or "255,0,0"
                                if content ~= "" then content = content .. "\n" end
                                content = content .. "<color rgb('" .. hpColor .. "')>HP: " .. hp .. "/" .. maxHp .. "</color>"
                            end
                            tag.InfoLabel.Text = content
                            tag.Enabled = (content ~= "")
                        end
                    end
                end)
            end
        end
    end
end)

-- C. èƒ½é‡åŒæ­¥èˆ‡æ‡¸æµ®çƒ
local ScreenGui = Instance.new("ScreenGui", game:GetService("CoreGui"))
local FloatIcon = Instance.new("ImageButton", ScreenGui)
FloatIcon.Size = UDim2.new(0, 60, 0, 60); FloatIcon.Position = UDim2.new(0.05, 0, 0.4, 0)
FloatIcon.BackgroundColor3 = Color3.fromRGB(20, 20, 20); FloatIcon.Image = "rbxassetid://4483345906"
FloatIcon.Draggable = true; local UIStroke = Instance.new("UIStroke", FloatIcon)
UIStroke.Thickness = 3; UIStroke.Color = Color3.fromRGB(0, 170, 255)
Instance.new("UICorner", FloatIcon).CornerRadius = UDim.new(1, 0)

task.spawn(function()
    local MainFrame = nil
    while not MainFrame do
        for _, v in pairs(game:GetService("CoreGui"):GetChildren()) do
            if v:IsA("ScreenGui") and (v.Name == "Luna" or v:FindFirstChild("Main")) then MainFrame = v:FindFirstChild("Main") or v:FindFirstChildOfClass("Frame") end
        end
        task.wait(1)
    end
    FloatIcon.MouseButton1Click:Connect(function() MainFrame.Visible = true; FloatIcon.Visible = false end)
    while true do
        FloatIcon.Visible = not (MainFrame and MainFrame.Visible)
        pcall(function() if _G.EnergyValue then game.Players.LocalPlayer.Character.Energy.Value = _G.EnergyValue end end)
        task.wait(0.5)
    end
end)

Luna:Notification({Title = "ä¿®å¾©å®Œç•¢", Content = "æ‰€æœ‰åŠŸèƒ½å·²æ›´æ–°ï¼", Duration = 3})
game.StarterGui:SetCore("SendNotification", {Title="è…³æœ¬å·²è¼‰å…¥!"; Text="ç”±å¯è»’è£½ä½œ"; Duration=5;})
