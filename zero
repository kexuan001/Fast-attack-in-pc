Tab1:CreateButton({Name = "é£›è¡Œ", Callback = function() loadstring(game:HttpGet("https://raw.githubusercontent.com/dt106101-ai/fly-gui-by-kexuan/refs/heads/main/README.md"))() end})
Tab1:CreateButton({Name = "å¿«é€Ÿæ”»æ“Š (Fast Attack)", Callback = function() _G.FastAttack = true; loadstring(game:HttpGet("https://raw.githubusercontent.com/kexuan001/Fast-attack-in-pc/refs/heads/main/thes%20fast"))() end})

