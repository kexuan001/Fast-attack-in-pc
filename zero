local Tab2 = Window:CreateTab({Name = "æˆ°é¬¥æ¨¡çµ„"})
local Tab3 = Window:CreateTab({Name = "åŠŸèƒ½/é€è¦–(å¾…é–‹ç™¼)"})
local Tab4 = Window:CreateTab({Name = "è¦–è¦ºè¨­å®š(å¾…é–‹ç™¼)"})
